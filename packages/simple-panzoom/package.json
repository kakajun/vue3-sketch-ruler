{
  "name": "simple-panzoom",
  "version": "2.0.2",
  "type": "module",
  "author": "kakajun <253495832@qq.com>",
  "private": false,
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/panzoom.js",
  "types": "lib/src/panzoom.d.ts",
  "exports": {
    ".": {
      "types": "./lib/src/panzoom.d.ts",
      "import": "./lib/panzoom.js",
      "require": "./lib/panzoom.cjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kakajun/vue3-sketch-ruler/packages/simple-panzoom"
  },
  "keywords": [
    "dom",
    "pan",
    "zoom"
  ],
  "scripts": {
    "test": "vitest run",
    "build": "tsup src/panzoom.ts --format esm,cjs --treeshake --clean --out-dir lib && tsc -p tsconfig.json",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "vitest": "^4.0.10"
  }
}
